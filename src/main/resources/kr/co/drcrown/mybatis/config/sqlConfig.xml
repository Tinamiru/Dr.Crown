<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

  <configuration>
  
  	<settings>
	 	<!-- DB 컬럼명을 낙타표기법으로 속성명과 자동 매핑하도록 설정하기 -->
	 	<setting name="mapUnderscoreToCamelCase" value="true"/>
	 	<setting name="jdbcTypeForNull" value="NULL" />
	 	<setting name="logImpl" value="LOG4J"/>
 	</settings>
  	  	
  	<typeAliases>
  		<typeAlias type="java.lang.String" alias="str" />
		<typeAlias type="java.lang.Integer" alias="int" />
		
		<typeAlias type="kr.co.drcrown.dto.MenuVO"  alias="menu"/>
		<typeAlias type="kr.co.drcrown.dto.MinfoVO"  alias="minfo"/>
 		<typeAlias type="kr.co.drcrown.dto.MemberVO"  alias="member"/>
		<typeAlias type="kr.co.drcrown.dto.NoticeVO"  alias="notice"/>
		<typeAlias type="kr.co.drcrown.dto.BookingVO"  alias="booking"/>
		<typeAlias type="kr.co.drcrown.dto.PatientVO"  alias="patient"/>
		<typeAlias type="kr.co.drcrown.dto.ApprovalVO"  alias="approval"/>
		<typeAlias type="kr.co.drcrown.dto.StockVO"  alias="stock"/>
		<typeAlias type="kr.co.drcrown.dto.CureDetailVO"  alias="curedetail"/>
		<typeAlias type="kr.co.drcrown.dto.MpriceVO"  alias="mprice"/>
		<typeAlias type="kr.co.drcrown.dto.CureVO"  alias="cure"/>
		<typeAlias type="kr.co.drcrown.dto.SymptomVO"  alias="symptom"/>
		<typeAlias type="kr.co.drcrown.dto.PtPayVO"  alias="ptpay"/>
		<typeAlias type="kr.co.drcrown.dto.CureDlistVO"  alias="curedlist"/>
		<typeAlias type="kr.co.drcrown.dto.FileVO"  alias="file"/>
		<typeAlias type="kr.co.drcrown.dto.DrugInfoVO"  alias="drugInfo"/>
		
		
  	</typeAliases>
    
	
  </configuration>
  
  
  
  
  
  
  